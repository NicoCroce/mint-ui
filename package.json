{
    "name": "tm-components",
    "version": "0.1.1",
    "description": "react, react-ui, react-base-components, react-library, react-ui-library",
    "author": "Kushal V. Mahajan",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": "https://gitlab.com/turtlemint/tm-components.git",
    "dependencies": {
        "@types/jest": "24.0.15",
        "@types/node": "12.6.8",
        "@types/react": "16.8.23",
        "@types/react-dom": "16.8.4",
        "connected-react-router": "6.4.0",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "immutable": "4.0.0-rc.12",
        "lodash": "4.17.14",
        "query-string": "^6.8.1",
        "react": "16.8.6",
        "react-dom": "16.8.6",
        "react-redux": "7.1.0-rc.1",
        "react-router": "5.0.1",
        "react-router-dom": "5.0.1",
        "react-scripts": "3.0.1",
        "redux": "4.0.1",
        "redux-immutable": "4.0.0",
        "redux-observable": "1.1.0",
        "rxjs": "6.5.2",
        "styled-components": "4.2.1",
        "tm-components": "https://gitlab.com/turtlemint/tm-components.git",
        "typescript": "3.5.3"
    },
    "devDependencies": {
        "@storybook/addon-knobs": "^5.1.9",
        "@testing-library/jest-dom": "^4.0.0",
        "@testing-library/react": "8.0.1",
        "@types/lodash": "^4.14.133",
        "@types/react-redux": "^7.1.1",
        "@types/react-router-dom": "^4.3.3",
        "@types/storybook__addon-knobs": "^5.0.3",
        "@types/styled-components": "4.1.16",
        "env-cmd": "9.0.2",
        "eslint-config-prettier": "^6.0.0",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-prettier": "^3.1.0",
        "husky": "^2.3.0",
        "prettier": "^1.18.2",
        "react-hooks-testing-library": "^0.5.1",
        "react-test-renderer": "^16.8.6",
        "source-map-explorer": "^2.0.1"
    },
    "publishConfig": {
        "registry": "http://nexus.turtlemint.com/repository/npm-private/"
    },
    "scripts": {
        "start": "env-cmd -e development react-scripts start",
        "build": "env-cmd -e production react-scripts build",
        "build:staging": "env-cmd -e staging react-scripts build",
        "test": "react-scripts test",
        "lint": "eslint './src/**/*.{ts,tsx}'",
        "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "storybook": "start-storybook -p 9001 -c .storybook"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "plugin:jsx-a11y/recommended"
        ],
        "plugins": [
            "jsx-a11y"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "sideEffects": [
        "./src/setupTests"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "yarn run lint && yarn run lint:fix"
        }
    },
    "files": [
        "README.MD",
        "dist/**"
    ],
    "babel": {
        "presets": [
            "react-app"
        ]
    }
}
