{
    "name": "@turtlemint/tm-components-web-test",
    "version": "1.0.26",
    "description": "react, react-ui, react-base-components, react-library, react-ui-library",
    "author": "Kushal V. Mahajan",
    "license": "MIT",
    "main": "index.js",
    "types": "index.d.ts",
    "repository": "https://gitlab.com/turtlemint/tm-components.git",
    "publishConfig": {
        "access": "public",
        "directory": "dist"
    },
    "dependencies": {
        "query-string": "^6.8.1",
        "react-scripts": "3.1.2"
    },
    "devDependencies": {
        "env-cmd": "9.0.2",
        "react-app-rewire-yarn-workspaces": "^1.0.3",
        "react-app-rewired": "^2.1.3",
        "source-map-explorer": "^2.0.1"
    },
    "scripts": {
        "start": "env-cmd -e development react-app-rewired start",
        "build": "env-cmd -e production react-app-rewired build",
        "build:staging": "env-cmd -e staging react-app-rewired build",
        "bundle": "rm -rf dist & tsc -p ./tsconfig-lib.json",
        "test": "react-app-rewired test",
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "storybook": "start-storybook -p 9001 -c .storybook",
        "copy": "cp README.MD package.json dist/",
        "prepublishOnly": "yarn run bundle; yarn run copy"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "sideEffects": [
        "./src/setupTests"
    ],
    "files": [
        "README.MD",
        "**"
    ]
}
